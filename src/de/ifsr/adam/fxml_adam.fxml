<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<TabPane fx:controller="de.ifsr.adam.FXMLAdamController" tabClosingPolicy="UNAVAILABLE" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <tabs>
    <Tab text="Import">
      <content>
            <GridPane alignment="CENTER" hgap="10.0" layoutX="2.0" layoutY="25.0" vgap="10.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints maxHeight="60.0" minHeight="0.0" prefHeight="37.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="60.0" minHeight="0.0" prefHeight="23.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <children>
                  <Button fx:id="selectFileBtn" mnemonicParsing="false" onAction="#handleSelectFileButtonAction" text="Select File" GridPane.columnIndex="1" />
                  <Button fx:id="selectFolderBtn" mnemonicParsing="false" text="Select Folder" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <TextField fx:id="selectImportFileTextField" />
                  <TextField GridPane.rowIndex="1" />
               </children>
            </GridPane>
      </content>
    </Tab>
    <Tab text="Diagram">
      <content>
            <GridPane alignment="CENTER" hgap="10.0" layoutX="2.0" layoutY="19.0" prefHeight="138.0" prefWidth="300.0" vgap="10.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints maxHeight="42.0" minHeight="10.0" prefHeight="35.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="48.0" minHeight="10.0" prefHeight="32.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <children>
                  <ChoiceBox prefWidth="150.0" />
                  <TextField GridPane.rowIndex="1" />
                  <Button mnemonicParsing="false" text="Select Report" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <Label text="Select Class" GridPane.columnIndex="1" />
                  <Button mnemonicParsing="false" text="Save Diagram" GridPane.columnIndex="1" GridPane.rowIndex="2" />
               </children>
            </GridPane>
      </content>
    </Tab>
      <Tab text="Report">
         <content>
            <ScrollBar orientation="VERTICAL" />
         </content>
      </Tab>
      <Tab text="Options">
        <content>
          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
        </content>
      </Tab>
  </tabs>
</TabPane>
